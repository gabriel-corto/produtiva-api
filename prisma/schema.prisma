generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id        Int     @id @default(autoincrement())
  name      String
  email     String  @unique
  password  String
  avatarUrl String?

  @@map("users")
}
